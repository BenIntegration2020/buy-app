<table
  id="productTable"
  class="table display table-bordered border border-dark border-2 m-3"
>
  <thead class="table-dark">
    <tr>
      <th>Code</th>
      <th>Description</th>
      <th>Price</th>
      <th>Edit / Delete</th>
    </tr>
  </thead>
  <tbody class="table-hover">
    <% console.log(products); %> 
    <% products.forEach((product)=> { %>
    <tr>
      <td><%= product.productCode %></td>
      <td><%= product.description %></td>
      <td><%= product.price %></td>
      <td>
        <button type="button">Edit</button><button type="button">Delete</button></td>
    </tr>
    <% }) %>
  </tbody>
</table>
